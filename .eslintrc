{
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-vars": 1,
    "flowtype-errors/show-errors": 2
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": [
    "flowtype-errors",
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "globals": {
  }
}
